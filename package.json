{
  "name": "file-template-yx",
  "displayName": "file_template_yx",
  "description": "create file template include react angular",
  "version": "0.0.3",
  "icon": "images/icon.png",
  "publisher": "wangjunjun",
  "author": {
    "name": "wangjunjun"
  },
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "file.react",
        "title": "create react file"
      },
      {
        "command": "file.angular",
        "title": "create angular file"
      }
    ],
    "keybindings": [
      {
        "command": "file.angular",
        "key": "ctrl+shift+a"
      }
    ],
     "configuration": {
            "properties": {
                "yx.name": {
                  "type": "string",
                  "description": "注释中的用户名"
                }
            }
     }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "cheerio": "^0.22.0",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
   "dependencies": {
    "@types/node": "^6.0.40",
    "cheerio": "^0.22.0",
    "vscode": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daxiajunjun/file-template"
  }
}
